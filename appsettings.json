{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information",
      "System": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_HOST};Database=${DB_DATABASE};Username=${DB_USERNAME};Password=${DB_PASSWORD};Port=${DB_PORT};"
  },
  "Jwt": {
    "Key": "${JwtKey}",
    "Issuer": "${JwtIssuer}",
    "Audience": "${JwtAudience}"
  },
  "AWS": {
    "Region": "ca-central-1",
    "S3BucketName": "chuckit-development"
  },
  "AllowedHosts": "*"
}